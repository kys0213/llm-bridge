{
  "name": "ollama-llama3-llm-bridge",
  "version": "0.0.1",
  "description": "Ollama Llama3 LLM Bridge - supports local Llama models through Ollama with type-safe configuration",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist esm && tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "test:ci": "vitest run --exclude='src/**/*.e2e.test.ts'",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run src/**/*.e2e.test.ts"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "llm-bridge-spec": "workspace:*",
    "zod": "^4.0.5",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.57.0",
    "ollama": "^0.5.15",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "vitest-mock-extended": "^3.1.0"
  },
  "peerDependencies": {
    "ollama": "^0.5.15",
    "llm-bridge-spec": "workspace:*",
    "zod": "^4.0.5"
  }
}
