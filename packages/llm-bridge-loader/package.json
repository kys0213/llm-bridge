{
  "name": "llm-bridge-loader",
  "version": "0.0.3",
  "description": "LLM Bridge Loader - load bridges from dependencies",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm build:clean && pnpm build:esm && pnpm build:cjs",
    "build:clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:ci": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "llm-bridge-spec": "workspace:*",
    "zod": "^4.0.5",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "openai-llm-bridge": "workspace:*"
  },
  "peerDependencies": {
    "llm-bridge-spec": "workspace:*",
    "zod": "^4.0.5"
  }
}
