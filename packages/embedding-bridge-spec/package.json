{
  "name": "embedding-bridge-spec",
  "version": "0.1.1",
  "private": false,
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "esm",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm build:clean && pnpm build:types && pnpm build:esm && pnpm build:cjs",
    "build:clean": "rimraf dist && rimraf esm",
    "build:types": "tsc -p tsconfig.types.json",
    "build:esm": "tsc -p tsconfig.esm.json && node ../../scripts/postbuild-esm.cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist && rimraf esm",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "llm-bridge-spec": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "llm-bridge-spec": "workspace:*"
  }
}
